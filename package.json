		"documentation": "^12.3.0",
		"esm": "^3.2.25",
		"fs-extra": "^9.0.0",
		"hirestime": "^6.0.1",
		"jest": "^24.9.0",
		"keyword-extractor": "0.0.18",
		"path": "^0.12.7",
		"rimraf": "^3.0.2",
		"rollup": "^1.32.1",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"walk-sync": "^2.1.0"
	},
